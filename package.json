{
  "name": "hedera-global-indexer",
  "version": "0.0.3",
  "type": "module",
  "description": "Hedera Global Indexer (dual-published ESM+CJS)",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "start": "tsx server.ts",
    "build": "tsup src/index.ts --dts --format esm,cjs --out-dir dist --clean",
    "dev": "tsup src/index.ts --dts --format esm,cjs --out-dir dist --watch --clean",
    "prepublishOnly": "npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "@xenova/transformers": "^2.17.2",
    "axios": "^1.11.0",
    "dotenv": "^16.6.1",
    "express": "^5.1.0",
    "node-fetch": "^2.7.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.12",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsup": "^8.1.0",
    "tsx": "^4.20.5",
    "typescript": "^5.5.4"
  }
}
